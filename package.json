{
	"name": "ember-cli-utilities",
	"version": "1.0.8",
	"description": "Utility Services / Mixins for ember",
	"keywords": [
		"ember-addon",
		"utilities",
		"services",
		"mixins"
	],
	"license": "MIT",
	"author": "Devonte Emokpae",
	"directories": {
		"doc": "doc",
		"test": "tests"
	},
	"repository": "https://github.com/devotox/ember-cli-utilities",
	"scripts": {
		"build": "ember build",
		"start": "ember server",
		"test": "ember try:each",
		"precommit": "lint-staged",
		"update": "ncu -u -a; yarn",
		"postrelease": "npm publish",
		"release": "npm version patch",
		"release-patch": "npm version patch",
		"release-minor": "npm version minor",
		"release-major": "npm version major",
		"postversion": "git push && git push --tags",
		"deploy:release": "npm run release; npm run deploy",
		"deploy:setup": "git add -A; git commit -m \"Added ember-cli-github-pages addon\"; git checkout --orphan gh-pages; rm -rf `bash -c \"ls -a | grep -vE '.gitignore|.git|node_modules|bower_components|([.]{1,2}/?$)'\"`; touch .gitkeep; git add -A; git commit -m \"initial gh-pages commit\"; git checkout master",
		"deploy": "ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push origin gh-pages; git checkout master",
		"format": "find app addon config tests -name '*.js' -type f | xargs prettier --write | xargs eslint --fix"
	},
	"lint-staged": {
		"*.{js,json,css}": [
			"prettier --write",
			"eslint --fix",
			"git add"
		]
	},
	"dependencies": {
		"babel-plugin-transform-object-rest-spread": "7.0.0-beta.3",
		"ember-cli-babel": "6.9.2",
		"ember-cli-sass": "7.1.1",
		"ember-cli-notifications": "4.2.1",
		"ember-notification-hub": "devotox/ember-notification-hub"
	},
	"devDependencies": {
		"broccoli-asset-rev": "2.6.0",
		"ember-ajax": "3.0.0",
		"ember-cli": "2.16.2",
		"ember-cli-dependency-checker": "2.1.0",
		"ember-cli-eslint": "4.2.2",
		"ember-cli-fastboot": "1.1.0",
		"ember-cli-github-pages": "0.1.2",
		"ember-cli-htmlbars": "2.0.3",
		"ember-cli-htmlbars-inline-precompile": "1.0.2",
		"ember-cli-inject-live-reload": "1.7.0",
		"ember-cli-moment-shim": "3.5.0",
		"ember-cli-qunit": "4.0.2",
		"ember-cli-shims": "1.2.0",
		"ember-cli-sri": "2.1.1",
		"ember-cli-uglify": "2.0.0",
		"ember-data": "2.17.0",
		"ember-disable-prototype-extensions": "1.1.3",
		"ember-export-application-global": "2.0.0",
		"ember-fetch": "3.4.3",
		"ember-i18n": "5.0.2",
		"ember-load-initializers": "1.0.0",
		"ember-moment": "7.5.0",
		"ember-resolver": "4.5.0",
		"ember-source": "2.17.0-beta.5",
		"eslint": "4.11.0",
		"eslint-config-prettier": "2.8.0",
		"eslint-plugin-ember-suave": "1.0.0",
		"eslint-plugin-prettier": "2.3.1",
		"loader.js": "4.6.0",
		"prettier": "1.8.2"
	},
	"engines": {
		"node": "^4.5 || 6.* || >= 7.*"
	},
	"ember-addon": {
		"configPath": "tests/dummy/config",
		"demoURL": "https://devotox.github.io/ember-cli-utilities"
	}
}
